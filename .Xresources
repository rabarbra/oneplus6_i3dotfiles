#define nord14 #A3BE8C
#define nord15 #B48EAD

#define CLEAR argb:00000000

#define BG        nord0
#define BG_TRANS  [80]#2E3440
#define BG_RGBA   rgba:D900/2E00/3400/4000
#define BG_ALT    nord3
#define TXT       nord4
#define TXT_ALT   nord6
#define PRIMARY   nord14
#define SECONDARY nord10
#define URGENT    nord11


*background      : BG
*background_trans: BG_TRANS
*background_alt  : BG_ALT
*foreground      : TXT
*primary         : PRIMARY
*secondary       : SECONDARY
*urgent          : URGENT

