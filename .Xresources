Xft.dpi: 192

;;URxvt.perl-ext-common:  resize-font,default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

urxvt*font:             xft:Inconsolata-g:style=g:pixelsize=28
urxvt*termName:         rxvt
urxvt*scrollBar:        false
urxvt*matcher.button:   1
urxvt*allow_bold:    true

Xft*antialias:          true
Xft*hinting:            full

#define nord0 #2E3440
